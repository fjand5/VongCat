<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vongcat">

    <application
        android:allowBackup="true"
        android:icon="@raw/logo"
        android:label="@string/app_name"
        android:roundIcon="@raw/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight">
        <activity
            android:name=".View.SupplyActivity"
            android:label="@string/title_activity_supply"
            android:theme="@style/NoActionBar"></activity>
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.AddOderActivity" />
        <activity android:name=".View.MoreActivity" />
        <activity android:name=".View.LogActivity" />
    </application>

</manifest>